{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "/index.html"
    },
    {
      "source": "/",
      "destination": "/gogobubbles-site/index.html"
    },
    {
      "source": "/booking",
      "destination": "/gogobubbles-site/booking.html"
    },
    {
      "source": "/feedback",
      "destination": "/gogobubbles-site/feedback.html"
    },
    {
      "source": "/jobs",
      "destination": "/gogobubbles-site/jobs.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "getgogobubbles.com"
        }
      ],
      "destination": "https://www.gogobubblesclean.com",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "getgogobubbles.com"
        }
      ],
      "destination": "https://www.gogobubblesclean.com",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "gogobubbleshouston.com"
        }
      ],
      "destination": "https://www.gogobubblesclean.com/jobs",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "gogobubbleshouston.com"
        }
      ],
      "destination": "https://www.gogobubblesclean.com/jobs",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "gogobubblesmobile.com"
        }
      ],
      "destination": "https://www.gogobubblesclean.com",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "gogobubblesmobile.com"
        }
      ],
      "destination": "https://www.gogobubblesclean.com",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "gogobubbles.com"
        }
      ],
      "destination": "https://www.gogobubblesclean.com",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "gogobubbles.com"
        }
      ],
      "destination": "https://www.gogobubblesclean.com",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
